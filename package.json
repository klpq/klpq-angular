{
  "name": "klpq-angular",
  "version": "0.1.0",
  "scripts": {
    "prettier": "prettier \"**/*\" --list-different --ignore-unknown",
    "prettier:fix": "prettier \"**/*\" --write --ignore-unknown",
    "build-env-file": "node build-env-file.js",
    "ng": "ng",
    "start": "yarn run build-env-file && ng serve --host=0.0.0.0",
    "build": "yarn run build-env-file && ng build --configuration production",
    "build:azure": "yarn run build-env-file && ./node_modules/.bin/ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-push": "yarn run prettier"
    }
  },
